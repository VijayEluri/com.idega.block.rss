<?xml version="1.0" encoding="UTF-8"?>

<project>
  <extend>${base.project.dir}/block-dependencies.xml</extend>
  <pomVersion>3</pomVersion>
  <name>idegaWeb RSS Block</name>
  <artifactId>com.idega.block.rss</artifactId>
  <currentVersion>3.1.0</currentVersion>
  <inceptionYear>2000</inceptionYear>
  <shortDescription>RSS</shortDescription>
  <versions>
    <version>
      <id>3.1.0</id>
      <name>3.1.0</name>
      <tag>RSS_3_1_0</tag>
    </version>
  </versions>
  <branches/>
  <developers>
    <developer>
      <name>Tryggvi Larusson</name>
      <id>tryggvil</id>
      <email>tryggvi@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
    <developer>
      <name>Pall Helgason</name>
      <id>palli</id>
      <email>palli@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
    <developer>
      <name>Eirikur Sveinn Hrafnsson</name>
      <id>eiki</id>
      <email>eiki@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
    <developer>
      <name>Thorhallur Helgason</name>
      <id>laddi</id>
      <email>laddi@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>bundles</groupId>
      <artifactId>com.idega.core</artifactId>
      <version>3.1.0</version>
      <type>jar</type>
      <url>http://www.idega.com/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://jdom.sourceforge.net/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>rome</groupId>
      <artifactId>rome</artifactId>
      <version>0.8</version>
      <type>jar</type>
      <url>https://rome.dev.java.net/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>rome</groupId>
      <artifactId>rome-fetcher</artifactId>
      <version>0.7</version>
      <type>jar</type>
      <url>https://rome.dev.java.net/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>bundles</groupId>
      <artifactId>com.idega.slide</artifactId>
      <version>3.1.0</version>
      <type>jar</type>
      <url>http://www.idega.com/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-httpclient</groupId>
      <artifactId>commons-httpclient</artifactId>
      <version>2.0.2</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/httpclient/</url>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
  </build>
  <properties/>
</project>
